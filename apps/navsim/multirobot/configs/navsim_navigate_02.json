      "disable_automatic_start": false
    },
    "simulation.scenario_manager": {
      "disable_automatic_start": true
    },
    "goals.goal_behavior": {
      "isaac.navigation.SelectorBehavior": {
        "desired_behavior": "patrol"
      }
    },
    "simulation.interface": {
      "input": {
        "port": 55021
      },
      "output": {
        "port": 55020,
        "host": "localhost"
      }
    },
